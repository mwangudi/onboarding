@model OnBoarding.ViewModels.ApplicationViewModel
@{
    ViewBag.Title = "New Application";
    Layout = "~/Views/Shared/_AdminNewLayout.cshtml";
}
<section class="section">
    <h5 class="section-header"> New Application </h5>
    <div class="row">
        <div class="col-12 col-sm-12 col-lg-12">
            <form id="form" enctype="multipart/form-data" method="post" class="needs-validation" novalidate="">
                <div class="list-group">
                    <div class="list-group-item py-3" data-acc-step>
                        <h5 class="mb-0" data-acc-title> Company Details </h5>
                        <div data-acc-content>
                            <div class="my-3 form-group form-row">
                                <div class="col-12 col-sm-6 col-lg-6">
                                    <label>Company Name: <span>*</span></label>
                                    <input type="text" id="CompanyName" name="CompanyName" placeholder="Company Name" value="@ViewData["CompanyName"]" class="form-control" />
                                    <input name="__RequestVerificationToken" type="hidden" value="<token_generated_here>">
                                    <input name="ClientID" id="ClientID" type="hidden" value="@ViewData["ClientID"]">
                                    <input name="CompanyEmail" id="CompanyEmail" type="hidden" value="@ViewData["CompanyEmail"]">
                                </div>
                                <div class="col-12 col-sm-6 col-lg-6">
                                    <label>Company Registration Number: <span>*</span></label>
                                    <input type="text" id="CompanyRegistration" name="CompanyRegistration" value="@ViewData["IncorporationNumber"]" placeholder="Company Registration Number" class="form-control required" />

                                </div>
                            </div>
                            <div class="my-3 form-group form-row">
                                <div class="col-12 col-sm-4 col-lg-4">
                                    <label>Company Building: <span>*</span></label>
                                    <input type="text" id="Building" name="Building" value="@ViewData["Building"]" placeholder="Company Building" class="form-control required" />
                                </div>
                                <div class="col-12 col-sm-4 col-lg-4">
                                    <label>Company Street: <span>*</span></label>
                                    <input type="text" id="Street" name="Street" value="@ViewData["Street"]" placeholder="Company Street" class="form-control required" />
                                </div>
                                <div class="col-12 col-sm-4 col-lg-4">
                                    <label>Company Town/City: <span>*</span></label>
                                    <input type="text" id="CompanyTownCity" name="CompanyTownCity" value="@ViewData["TownCity"]" placeholder="Company Town/City" class="form-control" />
                                </div>
                            </div>
                            <div class="my-3 form-group form-row">
                                <div class="col-12 col-sm-4 col-lg-4">
                                    <label>Business Email Address: <span>*</span></label>
                                    <input type="text" id="BusinessEmailAddress" name="BusinessEmailAddress" value="@ViewData["BusinessEmail"]" placeholder="Business Email" class="form-control required" />
                                </div>
                                <div class="col-12 col-sm-4 col-lg-4">
                                    <label>Atention To: <span>*</span></label>
                                    <input type="text" id="AttentionTo" name="AttentionTo" value="@ViewData["Attention"]" placeholder="Attention To" class="form-control required" />
                                </div>
                                <div class="col-12 col-sm-4 col-lg-4">
                                    <label>Company Fax:</label>
                                    <input type="text" id="CompanyFax" name="CompanyFax" value="@ViewData["Fax"]" placeholder="Company Fax" class="form-control" />
                                </div>
                                <div class="col-12 col-sm-4 col-lg-4">
                                    <label>Postal Address: <span>*</span></label>
                                    <input type="text" id="PostalAddress" name="PostalAddress" value="@ViewData["PostalAddress"]" placeholder="Postal Address" class="form-control required" />
                                </div>
                                <div class="col-12 col-sm-4 col-lg-4">
                                    <label>Postal Code: <span>*</span></label>
                                    <input type="text" id="PostalCode" name="PostalCode" value="@ViewData["PostalCode"]" placeholder="Postal Code" class="form-control required" />
                                </div>
                                <div class="col-12 col-sm-4 col-lg-4">
                                    <label>Town/City: <span>*</span></label>
                                    <input type="text" id="AddressTownCity" name="AddressTownCity" value="@ViewData["AddressTownCity"]" placeholder="Town/City" class="form-control required" />
                                    <input name="AcceptedTerms" id="AcceptedTerms" type="hidden" value="@ViewData["AcceptedTerms"]">
                                </div>
                            </div>
                            <div class="my-3 form-group form-row">
                                <div class="col-12 col-sm-3 col-lg-3">
                                    <button id="saving" class="btn btn-outline-primary d_none" type="button">
                                        <i class="fa fa-spinner fa-spin fa-2x fa-fw"></i> Saving Company Details ...
                                    </button>
                                    <button id="saved" class="btn btn-success d_none" type="button">
                                        <i class="fa fa-check-circle"></i> Company Details Saved
                                    </button>
                                    <button id="save" class="btn btn-outline-danger" type="button">
                                        <i class="fa fa-save"></i> Save
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="list-group-item py-3" data-acc-step>
                        <h5 class="mb-0" data-acc-title>Terms and Conditions</h5>
                        <div data-acc-content>
                            <div class="my-3 form-group">
                                <div class="col-12 col-sm-12 col-lg-12">
                                    <h6>This agreement is entered into by and between: Stanbic Bank Kenya Limited (acting only through its Global Markets Division unless stated otherwise in the agreement) (the “Bank”); and <b>@ViewData["CompanyName"]</b>.</h6>
                                    <div class="form-group scroll-terms">
                                        <embed id="scroll" src="~/Content/documents/Merged GTCs and eMT Agreement.pdf" width="900" height="500" alt="Terms and Conditions pdf" pluginspage="http://www.adobe.com/products/acrobat/readstep2.html" />
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-12 col-sm-6 col-lg-6"></div>
                                    <div class="col-12 col-sm-6 col-lg-6">
                                        <label for="checkbox1">I have read and hereby accept the above terms and conditions</label>
                                        <input value="false" type="checkbox" name="terms[]" id="terms">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="list-group-item py-3" data-acc-step>
                        <h5 class="mb-0" data-acc-title>Settlement Instructions</h5>
                        <div data-acc-content>
                            <div class="my-3 form-group">
                                <div class="col-12 col-sm-12 col-lg-12">
                                    <h6><small>The Settlement Instructions guides settlement once you book a deal.</small></h6>
                                    <h6><small>The accounts are credited or debited as per your deals. You should have a minimum of 2 accounts and a maximum of 5 </small></h6><hr class="nopadding" />
                                    <h6><b><small>Do you have a settlement account? <i> Indicate by choosing YES or NO below. </i></small></b></h6>
                                    <input name="SettlementAccountsCount" id="SettlementAccountsCount" type="hidden" value="@ViewBag.SettlementAccounts.Count">
                                    <input name="ClientSignatoryCount" id="ClientSignatoryCount" type="hidden" value="@ViewBag.ClientSignatory.Count">
                                    <input name="DesignatedUserCount" id="DesignatedUserCount" type="hidden" value="@ViewBag.DesignatedUser.Count">
                                </div>
                                <div id="SettlementAccountDiv" class="col-12 col-sm-3 col-lg-3">
                                    <select id="HaveSettlementAccount" name="HaveSettlementAccount" class="form-control select2 required" style="width: 100%;">
                                        <option value="" selected> Do you have a settlement Account? </option>
                                        <option value="Yes"> Yes </option>
                                        <option value="No"> No </option>
                                    </select>
                                </div><br />
                                <div id="HaveSettlementsDiv" class="col-12 col-sm-6 col-lg-6 d_none">
                                    @if (ViewBag.SettlementAccounts.Count > 0)
                                    {
                                        <table class="table table-bordered">
                                            <thead>
                                                <tr>
                                                    <th>Currency</th>
                                                    <th>Account Number</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var item in ViewBag.SettlementAccounts)
                                                {
                                                    <tr>
                                                        <td>@item.CurrencyName</td>
                                                        <td>@item.AccountNumber</td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    }
                                </div>
                            </div>
                            <div id="AccountDiv1" class="my-3 form-group form-row d_none">
                                <div class="col-12 col-sm-3 col-lg-3">
                                    <label>Select Currency <span>*</span></label>
                                    <select id="SelectCurrency1" name="SelectCurrency1" class="choose-currency form-control select2" style="width: 100%;"></select>
                                </div>
                                <div class="col-12 col-sm-3 col-lg-3 d_none" id="currencytypediv1">
                                    <label>Indicate Currency Type <span>*</span></label>
                                    <input type="text" class="form-control" name="InputCurrencyType1" id="InputCurrencyType1" placeholder="Currency" autocomplete="off" style="text-transform:uppercase">
                                </div>
                                <div class="col-12 col-sm-3 col-lg-3">
                                    <label>Account Number <span>*</span></label>
                                    <input type="text" class="form-control" name="SettlementAccount1" id="SettlementAccount1" placeholder="Account Number">
                                </div>
                            </div>
                            <div id="AccountDiv2" class="my-3 form-group form-row d_none">
                                <div class="col-12 col-sm-3 col-lg-3">
                                    <label>Select Currency <span>*</span></label>
                                    <select id="SelectCurrency2" name="SelectCurrency2" class="choose-currency form-control select2" style="width: 100%;"></select>
                                </div>
                                <div class="col-12 col-sm-3 col-lg-3 d_none" id="currencytypediv2">
                                    <label>Indicate Currency Type <span>*</span></label>
                                    <input type="text" class="form-control" name="InputCurrencyType2" id="InputCurrencyType2" placeholder="Indicate Currency Type" autocomplete="off">
                                </div>
                                <div class="col-12 col-sm-3 col-lg-3">
                                    <label>Account Number <span>*</span></label>
                                    <input type="text" class="form-control" name="SettlementAccount2" id="SettlementAccount2" placeholder="Account Number">
                                </div>
                                <div class="m_top_20">
                                    <button type="button" class="btn btn-success" style="margin-top: 22px;" id="btnAddAccount3"><i class="fa fa-plus"></i> Add Another Account</button>
                                </div>
                            </div>
                            <div id="AccountDiv3" class="my-3 form-group form-row d_none">
                                <div class="col-12 col-sm-3 col-lg-3">
                                    <label>Select Currency <span>*</span></label>
                                    <select id="SelectCurrency3" name="SelectCurrency3" class="choose-currency form-control select2" style="width: 100%;"></select>
                                </div>
                                <div class="col-12 col-sm-3 col-lg-3 d_none" id="currencytypediv3">
                                    <label>Indicate Currency Type <span>*</span></label>
                                    <input type="text" class="form-control" name="InputCurrencyType3" id="InputCurrencyType3" placeholder="Indicate Currency Type" autocomplete="off">
                                </div>
                                <div class="col-12 col-sm-3 col-lg-3">
                                    <label>Account Number <span>*</span></label>
                                    <input type="text" class="form-control" name="SettlementAccount3" id="SettlementAccount3" placeholder="Account Number">
                                </div>
                                <div class="m_top_20">
                                    <button type="button" class="btn btn-success" style="margin-top: 22px;" id="btnAddAccount4"><i class="fa fa-plus"></i> Add Another Account</button>
                                    <button type="button" class="btn btn-danger" style="margin-top: 22px;" id="btnRemoveAccount3"><i class="fa fa-trash"></i> Remove Account</button>
                                </div>
                            </div>
                            <div id="AccountDiv4" class="my-3 form-group form-row d_none">
                                <div class="col-12 col-sm-3 col-lg-3">
                                    <label>Select Currency <span>*</span></label>
                                    <select id="SelectCurrency4" name="SelectCurrency4" class="choose-currency form-control select2" style="width: 100%;"></select>
                                </div>
                                <div class="col-12 col-sm-3 col-lg-3 d_none" id="currencytypediv4">
                                    <label>Indicate Currency Type <span>*</span></label>
                                    <input type="text" class="form-control" name="InputCurrencyType4" id="InputCurrencyType4" placeholder="Indicate Currency Type" autocomplete="off">
                                </div>
                                <div class="col-12 col-sm-3 col-lg-3">
                                    <label>Account Number <span>*</span></label>
                                    <input type="text" class="form-control" name="SettlementAccount4" id="SettlementAccount4" placeholder="Account Number">
                                </div>
                                <div class="m_top_20">
                                    <button type="button" class="btn btn-success" style="margin-top: 22px;" id="btnAddAccount5"><i class="fa fa-plus"></i> Add Another Account</button>
                                    <button type="button" class="d_none btn btn-danger" style="margin-top: 22px;" id="btnRemoveAccount4"><i class="fa fa-trash"></i> Remove Account</button>
                                </div>
                            </div>
                            <div id="AccountDiv5" class="my-3 form-group form-row d_none">
                                <div class="col-12 col-sm-3 col-lg-3">
                                    <label>Select Currency <span>*</span></label>
                                    <select id="SelectCurrency5" name="SelectCurrency5" class="choose-currency form-control select2" style="width: 100%;"></select>
                                </div>
                                <div class="col-12 col-sm-3 col-lg-3 d_none" id="currencytypediv5">
                                    <label>Indicate Currency Type <span>*</span></label>
                                    <input type="text" class="form-control" name="InputCurrencyType5" id="InputCurrencyType5" placeholder="Indicate Currency Type" autocomplete="off">
                                </div>
                                <div class="col-12 col-sm-3 col-lg-3">
                                    <label>Account Number <span>*</span></label>
                                    <input type="text" class="form-control" name="SettlementAccount5" id="SettlementAccount5" placeholder="Account Number">
                                </div>
                                <div class="m_top_10">
                                    <button type="button" class="d_none btn btn-danger" style="margin-top: 22px;" id="btnRemoveAccount5"><i class="fa fa-trash"></i> Remove Account</button>
                                </div>
                            </div>
                            <div class="my-3 form-group form-row">
                                <div class="col-12 col-sm-3 col-lg-3">
                                    <button id="savingsettlement" class="btn btn-outline-primary d_none" type="button">
                                        <i class="fa fa-spinner fa-spin fa-2x fa-fw"></i> Saving Settlement Instructions ...
                                    </button>
                                    <button id="savedsettlement" class="btn btn-success d_none" type="button">
                                        <i class="fa fa-check-circle"></i> Settlement Instructions Saved
                                    </button>
                                    <button id="btn-save-settlement" class="btn btn-outline-danger" type="button">
                                        <i class="fa fa-save"></i> Save
                                    </button>
                                    <button id="btn-edit-settlement" class="btn btn-outline-info" type="button">
                                        <i class="fa fa-edit"></i> Edit Settlements
                                    </button>
                                </div>
                            </div><hr />
                            <div class="my-3 form-group form-row">
                                <h6 class="d_none" id="disclaimer"><i><small>The counterparty hereby indemnifies the Bank against all and any losses and any demages that the Bank may suffer as a result of settling any transaction in any of the aforementioned bank accounts.</small></i></h6>
                            </div>
                        </div>
                    </div>
                    <div class="list-group-item py-3" data-acc-step>
                        <h5 class="mb-0" data-acc-title>Account Signatories</h5>
                        <div data-acc-content>
                            <div class="my-3 form-group">
                                <div class="col-12 col-sm-12 col-lg-12">
                                    <h6><small>The following representatives of the counterparty are hereby designated as Authorized Signatories of the counterparty, having the authority to sign any relevant document pertaining to these terms on behalf of the Counterparty.</small></h6>
                                </div>
                            </div>
                            <div id="SignatoriesDiv" class="col-12 col-sm-12 col-lg-12 d_none">
                                @if (ViewBag.ClientSignatory.Count > 0)
                                {
                                    <table class="table table-bordered">
                                        <thead>
                                            <tr>
                                                <th> Full Names</th>
                                                <th> Designation </th>
                                                <th> Phone </th>
                                                <th> Email </th>
                                                <th> Uploaded Signature </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var item in ViewBag.ClientSignatory)
                                            {
                                                <tr>
                                                    <td>@(item.Surname + " " + item.OtherNames)</td>
                                                    <td>@item.Designation</td>
                                                    <td>@item.PhoneNumber</td>
                                                    <td>@item.EmailAddress</td>
                                                    <td>
                                                        @if (item.Signature == null || item.Signature == "")
                                                        {
                                                            <img width="100" height="80" src="~/Content/images/signatures/no-signature-17.jpg" />
                                                        }
                                                        else
                                                        {
                                                            <img width="100" height="80" src="~/Content/images/signatures/@item.Signature" />
                                                        }
                                                    </td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                }
                            </div>
                            <div id="SignatoryDiv1" class="my-3">
                                <div class="card">
                                    <div class="card-header">
                                        <div class="alert alert-primary mb-0 t_align_c">  Add signatories as per your account mandate </div><hr />
                                        <i class="fa fa-user-circle"></i> Account Signatory 1: <i>[My Details]</i>
                                    </div>
                                    <div class="card-body form-group form-row">

                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Surname <span>*</span></label>
                                            <input type="text" class="form-control required" name="SignatorySurname1" id="SignatorySurname1" value="@ViewData["Surname"]" placeholder="Surname">
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Other Names <span>*</span></label>
                                            <input type="text" class="form-control required" name="SignatoryOtherNames1" id="SignatoryOtherNames1" value="@ViewData["OtherNames"]" placeholder="Other Names">
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Designation: <span>*</span></label>
                                            <input type="text" class="form-control required" name="SignatoryDesignation1" id="SignatoryDesignation1" placeholder="Signatory Designation">
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Email Address: <span>*</span></label>
                                            <input type="email" class="form-control required textToLower" name="SignatoryEmail1" id="SignatoryEmail1" value="@ViewData["CompanyEmail"]" placeholder="Email Address" readonly />
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Phone Number: <span>*</span></label>
                                            <input type="text" class="form-control required" name="SignatoryPhoneNumber1" id="SignatoryPhoneNumber1" placeholder="Include Country Code">
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3 imgUp">
                                            <div class="imagePreview"></div>
                                            <label class="btn btn-primary" style="color:#fff">
                                                Upload Signature <input id="inputFile" name="inputFile" type="file" class="uploadFile img" value="Upload Signature" style="width: 0px;height: 0px;overflow: hidden;">
                                            </label>
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3" style="margin-top:55px; line-height:15px;">
                                            <h6><small>Image types to be uploaded</small></h6>
                                            <ul>
                                                <li style="color:red"><small>JPEG Image (.jpeg)</small></li>
                                                <li style="color:red"><small>JPEG Image (.jpg)</small></li>
                                                <li style="color:red"><small>Portable Network Graphic (.png)</small></li>
                                            </ul>
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <button type="button" class="btn btn-success" style="margin-top: 110px;" id="btnAddSignatory2"><i class="fa fa-plus"></i> Add Signatory</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="SignatoryDiv2" class="my-3 d_none">
                                <div class="card">
                                    <div class="card-header">
                                        <i class="fa fa-user-circle"></i> Account Signatory 2:
                                    </div>
                                    <div class="card-body form-group form-row">
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Surname <span>*</span></label>
                                            <input type="text" class="form-control" name="SignatorySurname2" id="SignatorySurname2" placeholder="Surname">
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Other Names <span>*</span></label>
                                            <input type="text" class="form-control" name="SignatoryOtherNames2" id="SignatoryOtherNames2" placeholder="Other Names">
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Designation: <span>*</span></label>
                                            <input type="text" class="form-control" name="SignatoryDesignation2" id="SignatoryDesignation2" placeholder="Signatory Designation">
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Email Address: <span>*</span></label>
                                            <input type="email" class="form-control textToLower" name="SignatoryEmail2" id="SignatoryEmail2" placeholder="Email Address">
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Phone Number: <span>*</span></label>
                                            <input type="text" class="form-control" name="SignatoryPhoneNumber2" id="SignatoryPhoneNumber2" placeholder="Include Country Code">
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <button type="button" style="margin-top:25px;" class="btn btn-danger w_full" id="btnRemoveSignatory2"><i class="fa fa-trash"></i> Remove </button>
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <button type="button" style="margin-top:25px;" class="btn btn-success w_full" id="btnAddSignatory3"><i class="fa fa-plus"></i> Add Signatory</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="SignatoryDiv3" class="my-3 d_none">
                                <div class="card">
                                    <div class="card-header">
                                        <i class="fa fa-user-circle"></i> Account Signatory 3:
                                    </div>
                                    <div class="card-body form-group form-row">
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Surname <span>*</span></label>
                                            <input type="text" class="form-control" name="SignatorySurname3" id="SignatorySurname3" placeholder="Surname">
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Other Names <span>*</span></label>
                                            <input type="text" class="form-control" name="SignatoryOtherNames3" id="SignatoryOtherNames3" placeholder="Other Names">
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Designation: <span>*</span></label>
                                            <input type="text" class="form-control" name="SignatoryDesignation3" id="SignatoryDesignation3" placeholder="Signatory Designation">
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Email Address: <span>*</span></label>
                                            <input type="email" class="form-control textToLower" name="SignatoryEmail3" id="SignatoryEmail3" placeholder="Email Address">
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <label>Phone Number: <span>*</span></label>
                                            <input type="text" class="form-control" name="SignatoryPhoneNumber3" id="SignatoryPhoneNumber3" placeholder="Include Country Code">
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <button type="button" style="margin-top:25px;" class="btn btn-danger w_full" id="btnRemoveSignatory3"><i class="fa fa-trash"></i> Remove </button>
                                        </div>
                                        <div class="col-12 col-sm-3 col-lg-3">
                                            <button type="button" style="margin-top:25px;" class="btn btn-success w_full" id="btnAddSignatory4"><i class="fa fa-plus"></i> Add Signatory</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="SignatoryDiv4" class="my-3 d_none">
                                <div class="card">
                                    <div class="card-header">
                                        <i class="fa fa-user-circle"></i> Account Signatory 4:
                                    </div>
                                    <div class="card-body form-group form-row">
