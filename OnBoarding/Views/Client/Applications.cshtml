			e.preventDefault();
			if ($('#ApplicationForm').valid()) {
				var vidFileLength = $("#inputFile")[0].files.length;
				if (vidFileLength === 0) {
					toastr.error('Upload your scanned signature: Image should be of size(100 by 80)');
				}
				else {
					$("#Loading_Div").show('fast');
					$('#ApplicationForm').hide("fast");
					var formData = new FormData($('#ApplicationForm')[0]);
					$.ajax({
						url: '@Url.Action("SaveSignatoriesInfo", "Client")',
						type: 'POST',
						data: formData,
						async: true,
						success: function () {
							window.setTimeout(saved, 500);
							function saved() {
								$("#Loading_Div").hide('fast');
								$('#ApplicationForm').show("fast");
								toastr.success('Your application has been saved');
							}
						},
						cache: false,
						contentType: false,
						processData: false
					});
				}
			}
			else {
				toastr.error('Please correct the errors on form inputs to proceed');
			}
		});

		//RepresentativesDiv
		var representatives = '@ViewBag.DesignatedUser.Count';
		if (representatives > 0)
		{
			$('#UserDiv1').addClass('d_none');
			$('.btn-save-representatives').addClass('d_none');
			$('.btn-edit-representatives').removeClass('d_none');
			$('#UserSurname1').removeClass('required');
			$('#UserMobileNumber1').removeClass('required');  
			$('#UserOthernames1').removeClass('required');
			$('#UserEmail1').removeClass('required');
		}
		else
		{
			$('#UserDiv1').removeClass('d_none');
			$('.btn-save-representatives').removeClass('d_none');
			$('.btn-edit-representatives').addClass('d_none');
		}

		//Edit Representatives
		$('.form-wizard .btn-edit-representatives').on('click', function (e) {
			e.preventDefault();
			$("#Loading_Div").show('fast');
			$('#ApplicationForm').hide("fast");
			$.ajax({
				url: '@Url.Action("ClearRepresentatives", "Client")',
				type: 'POST',
				async: true,
				success: function () {
					window.setTimeout(saved, 500);
					function saved() {
						$("#Loading_Div").hide('fast');
						$('#ApplicationForm').show("fast");
						$('#inputFile').removeClass('required');
						$('#UserDiv1').removeClass('d_none');
						$('.btn-save-representatives').removeClass('d_none');
						$('.btn-edit-representatives').addClass('d_none');
						$('#representatives-table').addClass('d_none');
					}
				},
				cache: false,
				contentType: false,
				processData: false
			});
		});

		//Save Representatives
		$('.form-wizard .btn-save-representatives').on('click', function (e) {
			e.preventDefault();
			if ($('#ApplicationForm').valid()) {
				$("#Loading_Div").show('fast');
				$('#ApplicationForm').hide("fast");
				var formData = new FormData($('#ApplicationForm')[0]);
				$.ajax({
					url: '@Url.Action("SaveRepresentativesInfo", "Client")',
					type: 'POST',
					data: formData,
					async: true,
					success: function () {
						window.setTimeout(saved, 500);
						function saved() {
							$("#Loading_Div").hide('fast');
							$('#ApplicationForm').show("fast");
							toastr.success('Your application has been saved');
						}
					},
					cache: false,
					contentType: false,
					processData: false
				});
			}
			else {
				toastr.error('Please correct the errors on form inputs to proceed');
			}
		});

		//Submit Form Action
		$("form#ApplicationForm").submit(function (e) {
			e.preventDefault();
			if ($('#ApplicationForm').valid()) {
				$("#Loading_Div").show('fast');
				$('#ApplicationForm').hide("fast");
				var formData = new FormData($(this)[0]);
				$.ajax({
					url: '@Url.Action("AddNewApplication", "Client")',
					type: 'POST',
					data: formData,
					async: true,
					success: function () {
						window.setTimeout(close, 500);
						window.setTimeout(close2, 1500);
						function close() {
							$('#ApplicationForm')[0].reset();
							$("#Loading_Div").hide('fast');
							$('#Success_Div').show("fast");
						}
						function close2() {
							$('#Success_Div').hide("fast");
							window.location.replace("/Client/ViewAll");
						}
					},
					cache: false,
					contentType: false,
					processData: false
				});
			}
		});

	});
</script>
<script type="text/javascript">
	var IDLE_TIMEOUT = 600; //seconds 5 minutes
	var _idleSecondsCounter = 0;
	document.onclick = function () {
		_idleSecondsCounter = 0;
	};
	document.onmousemove = function () {
		_idleSecondsCounter = 0;
	};
	document.onkeypress = function () {
		_idleSecondsCounter = 0;
	};
	window.setInterval(CheckIdleTime, 1000);
	function CheckIdleTime() {
		_idleSecondsCounter++;
		var oPanel = document.getElementById("SecondsUntilExpire");
		if (oPanel)
			oPanel.innerHTML = (IDLE_TIMEOUT - _idleSecondsCounter) + "";
		if (_idleSecondsCounter >= IDLE_TIMEOUT) {
			document.getElementById('logoutForm').submit()
		}
	}
</script>