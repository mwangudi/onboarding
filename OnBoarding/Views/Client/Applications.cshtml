@model OnBoarding.ViewModels.ApplicationViewModel
@{
	ViewBag.Message1 = "Welcome to the Global Markets Onboarding Portal. Let’s get started!";
	ViewBag.Message2 = "Please read the terms and conditions carefully before accepting them.";
	ViewBag.Message3 = "The Settlement Instructions guides settlement once you book a deal. The accounts are credited or debited as per your deals. You should have a minimum of 2 accounts and a maximum of 5";
	ViewBag.Message4 = "Kindly input signatories as per mandate held in the Bank";
	ViewBag.Message5 = "You are almost there. Please let us know who has approval to deal a rate on behalf of the company";
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<!-- /.row -->
<div class="row">
	<ul class="nav nav-tabs">
		<li class="active pull-left"><a data-toggle="tab" href="#secondTab"> New Application </a></li>
	</ul>
	<div class="tab-content">
		<div id="secondTab" class="tab-pane fade in active">
			<div class="col-md-12 form-wizard">
				<!-- Form Wizard -->
				<form role="form" id="ApplicationForm" action="" method="POST" accept-charset="utf-8">
					<p>Fill the required form fields (*) to go to the next step </p>
					<!-- Form progress -->
					<div class="form-wizard-steps form-wizard-total-steps-6">
						<div class="form-wizard-progress">
							<div class="form-wizard-progress-line" data-now-value="12.25" data-number-of-steps="6" style="width: 12.25%;"></div>
						</div>
						<!-- Step 1 -->
						<div class="form-wizard-step active">
							<div class="form-wizard-step-icon"><i class="fa fa-user" aria-hidden="true"></i></div>
							<p>Company Details</p>
						</div>
						<!-- Step 1 -->
						<!-- Step 2-->
						<div class="form-wizard-step">
							<div class="form-wizard-step-icon"><i class="fa fa-check" aria-hidden="true"></i></div>
							<p> Terms and Conditions</p>
						</div>
						<!-- Step 2 -->
						<!-- Step 3 -->
						<div class="form-wizard-step">
							<div class="form-wizard-step-icon"><i class="fa fa-bank" aria-hidden="true"></i></div>
							<p>Settlement Instructions</p>
						</div>
						<!-- Step 3 -->
						<!-- Step 4 -->
						<div class="form-wizard-step">
							<div class="form-wizard-step-icon"><i class="fa fa-user-md" aria-hidden="true"></i></div>
							<p>Account Signatories</p>
						</div>
						<!-- Step 4 -->
						<!-- Step 5 -->
						<div class="form-wizard-step">
							<div class="form-wizard-step-icon"><i class="fa fa-users" aria-hidden="true"></i></div>
							<p>Authorized representatives</p>
						</div>
						<!-- Step 5 -->
						<!-- Step 6 -->
						<div class="form-wizard-step">
							<div class="form-wizard-step-icon"><i class="fa fa-file-text" aria-hidden="true"></i></div>
							<p>Confirm</p>
						</div>
						<!-- Step 6 -->
					</div>
					<!-- Form progress -->
					<!-- Form Step 1 -->
					<fieldset>
						<h4> Company Details <span>Step 1 of 6</span></h4>
						<p>Provide your Company Details</p><hr />
						<div class="form-group">
							<table class="table table-hover">
								<tr class="success">
									<th>Company Details:</th>
								</tr>
							</table>
							<div class="row">
								<div class="col-md-4">
									<label> Company Name:</label>
									<input type="text" class="form-control" name="CompanyName" id="CompanyName" value="@ViewData["CompanyName"]" placeholder="Company / Individual Name">
									<input name="__RequestVerificationToken" type="hidden" value="<token_generated_here>">
									<input name="ClientID" id="ClientID" type="hidden" value="@ViewData["ClientID"]">
									<input name="CompanyEmail" id="CompanyEmail" type="hidden" value="@ViewData["CompanyEmail"]">
								</div>
								<div class="col-md-4">
									<label>Company Registration Number: <span>*</span></label>
									<input type="text" class="form-control required" name="CompanyRegistration" id="CompanyRegistration" value="@ViewData["IncorporationNumber"]" placeholder="Company Registration Number">
								</div>
							</div>
							<table class="table table-hover m_top_10">
								<tr class="success">
									<th>Principal Place of Business:</th>
								</tr>
							</table>
							<div class="row">
								<div class="col-md-4">
									<label>Company Building: <span>*</span></label>
									<input type="text" id="Building" name="Building" value="@ViewData["Building"]" placeholder="Building" class="form-control required">
								</div>
								<div class="col-md-4">
									<label>Company Street: <span>*</span></label>
									<input type="text" id="Street" name="Street" placeholder="Street" value="@ViewData["Street"]" class="form-control required">
								</div>
								<div class="col-md-4">
									<label>Company Town/City: <span>*</span></label>
									<input type="text" id="CompanyTownCity" name="CompanyTownCity" placeholder="Company Town / City" value="@ViewData["TownCity"]" class="form-control required">
								</div>
							</div>
							<table class="table table-hover m_top_10">
								<tr class="success">
									<th>Address where notices will be sent to the counterparty:</th>
								</tr>
							</table>
							<div class="row">
								<div class="col-md-4">
									<label>Business Email Address: <span>*</span></label>
									<input type="email" id="BusinessEmailAddress" name="BusinessEmailAddress" value="@ViewData["BusinessEmail"]" placeholder="Business Email Address" class="form-control required">
								</div>
								<div class="col-md-4">
									<label>Attention to: <span>*</span></label>
									<input type="text" id="AttentionTo" name="AttentionTo" value="@ViewData["Attention"]" placeholder="Attention Person" class="form-control required">
								</div>
								<div class="col-md-4">
									<label>Company Fax:</label>
									<input type="text" id="CompanyFax" name="CompanyFax" value="@ViewData["Fax"]" placeholder="Company Fax" class="form-control">
								</div>
							</div>
							<div class="row">
								<div class="col-md-4">
									<label>Postal Address: <span>*</span></label>
									<input type="text" id="PostalAddress" name="PostalAddress" placeholder="Postal Address" value="@ViewData["PostalAddress"]" class="form-control required">
								</div>
								<div class="col-md-4">
									<label>Postal Code: <span>*</span></label>
									<input type="text" id="PostalCode" name="PostalCode" placeholder="Postal Code" value="@ViewData["PostalCode"]" class="form-control required">
								</div>
								<div class="col-md-4">
									<label> Town/City: <span>*</span></label>
									<input type="text" id="AddressTownCity" name="AddressTownCity" placeholder="Town / City" value="@ViewData["TownCity"]" class="form-control required">
								</div>
							</div>
							<div class="row">
								<div class="col-md-4 pull-right m_top_20">
									<div class="form-wizard-buttons">
										<button type="button" class="btn btn-save"><i class="fa fa-save"></i> Save</button>
										<button type="button" class="btn btn-next  btn-fao"> Next <i class="fa fa-angle-right"></i></button>
									</div>
								</div>
							</div>
						</div>
					</fieldset>
					<!-- Form Step 1 -->
					<!-- Form Step 2 -->
					<fieldset>
						<h4>General Terms and Conditions <span> Step 2 - 6 </span></h4>
						<h5>This agreement is entered into by and between: Stanbic Bank Kenya Limited (acting only through its Global Markets Division unless stated otherwise in the agreement) (the “Bank”); and <b>@ViewData["CompanyName"]</b>.</h5>
						<div class="form-group scroll-terms">
							<embed id="scroll" src="~/Content/documents/Merged GTCs and eMT Agreement.pdf" width="950" height="500" alt="Terms and Conditions pdf" pluginspage="http://www.adobe.com/products/acrobat/readstep2.html" />
						</div>
						<div class="form-group">
							<div class="col-md-8">
								<div class="input-group accept-terms">
									<input type="text" id="error-check" class="form-control">
									<span class="input-group-addon beautiful" id="beautifulcheckbox">
										<input value="true" type="checkbox" id="terms" name="terms" class="form-control">
									</span>
								</div>
							</div>
							<div class="col-md-4">
								<div class="form-wizard-buttons">
									<button type="button" class="btn btn-previous"><i class="fa fa-angle-left"></i> Back</button>
									<button type="button" class="btn btn-twende-mbele" id="acceptterms">Next <i class="fa fa-angle-right"></i></button>
								</div>
							</div>
						</div><br /><br />
					</fieldset>
					<!-- Form Step 2 -->
					<!-- Form Step 3 -->
					<fieldset>
						<h4>Standard Settlement Instructions <span>Step 3 - 6</span></h4>
						<h5>The Settlement Instructions guides settlement once you book a deal.</h5>
						<h5>The accounts are credited or debited as per your deals. You should have a minimum of 2 accounts and a maximum of 8 </h5><hr />
						<div class="form-group">
							@if (ViewBag.SettlementAccounts.Count > 0)
							{
								int nombre = 1;
								<table class="custom-table" id="settlement-table">
									<h5><b> Clients Settlement Accounts. </b></h5>
									<th>#</th>
									<th> Currency </th>
									<th> Account Number </th>
									@foreach (var item in ViewBag.SettlementAccounts)
									{
										<tbody>
										<td>@nombre</td>
										<td>@item.CurrencyName</td>
										<td>@item.AccountNumber</td>
										</tbody>

										nombre = nombre + 1;
									}
								</table>
							}
							else
							{
								<h5><b> Indicate Settlement Accounts. </b></h5>
							}
							<div class="row" id="HaveSettlementsDiv">
								<h5 style="margin-left: 15px;"><b> Do you have a settlement account? <i> Indicate by choosing YES of NO below. </i></b></h5>
								<div class="col-md-3">
									<select id="HaveSettlementAccount" name="HaveSettlementAccount" class="form-control select2" style="width: 100%;">
										<option value="" selected> Do you have a settlement Account? </option>
										<option value="Yes"> Yes </option>
										<option value="No"> No </option>
									</select>
								</div>
							</div><br />
							<div class="row d_none" id="AccountDiv1">
								<div class="col-md-3">
									<label>Select Currency <span>*</span></label>
									<select id="SelectCurrency1" name="SelectCurrency1" class="choose-currency form-control select2" style="width: 100%;"></select>
								</div>
								<div class="col-md-3 d_none" id="currencytypediv1">
									<label>Indicate Currency Type <span>*</span></label>
									<input type="text" class="form-control" name="InputCurrencyType1" id="InputCurrencyType1" placeholder="Currency" autocomplete="off" style="text-transform:uppercase">
								</div>
								<div class="col-md-3">
									<label>Account Number <span>*</span></label>
									<input type="text" class="form-control" name="SettlementAccount1" id="SettlementAccount1" placeholder="Account Number">
								</div>
								<div class="d_none col-md-3 m_top_10">
									<button type="button" class="btn btn-success" style="margin: 8px 15px 0px 0px;" id="btnAddAccount2"><span class="glyphicon glyphicon-plus-sign"></span> Add Another Account</button>
								</div>
							</div>
							<div class="row d_none" id="AccountDiv2">
								<div class="col-md-3">
									<label>Select Currency <span>*</span></label>
									<select id="SelectCurrency2" name="SelectCurrency2" class="choose-currency form-control select2" style="width: 100%;"></select>
								</div>
								<div class="col-md-3 d_none" id="currencytypediv2">
									<label>Indicate Currency Type <span>*</span></label>
									<input type="text" class="form-control" name="InputCurrencyType2" id="InputCurrencyType2" placeholder="Indicate Currency Type" autocomplete="off">
								</div>
								<div class="col-md-3">
									<label>Account Number <span>*</span></label>
									<input type="text" class="form-control" name="SettlementAccount2" id="SettlementAccount2" placeholder="Account Number">
								</div>
								<div class="form-wizard-buttons m_top_20">
									<button type="button" class="btn btn-success" style="margin: 8px 15px 0px 0px;" id="btnAddAccount3"><span class="glyphicon glyphicon-plus-sign"></span> Add Another Account</button>
								</div>
							</div>
							<div class="row d_none" id="AccountDiv3">
								<div class="col-md-3">
									<label>Select Currency <span>*</span></label>
									<select id="SelectCurrency3" name="SelectCurrency3" class="choose-currency form-control select2" style="width: 100%;"></select>
								</div>
								<div class="col-md-3 d_none" id="currencytypediv3">
									<label>Indicate Currency Type <span>*</span></label>
									<input type="text" class="form-control" name="InputCurrencyType3" id="InputCurrencyType3" placeholder="Indicate Currency Type" autocomplete="off">
								</div>
								<div class="col-md-3">
									<label>Account Number <span>*</span></label>
									<input type="text" class="form-control" name="SettlementAccount3" id="SettlementAccount3" value="" placeholder="Account Number">
								</div>
								<div class="form-wizard-buttons m_top_20"> 
									<button type="button" class="btn btn-success" style="margin: 8px 15px 0px 0px;" id="btnAddAccount4"><span class="glyphicon glyphicon-plus-sign"></span> Add Another Account</button>
									<button type="button" class="btn btn-danger" style="margin: 8px 15px 0px 0px;" id="btnRemoveAccount3"><span class="glyphicon glyphicon-minus-sign"></span> Remove Account</button>
								</div>
							</div>
							<div class="row d_none" id="AccountDiv4">
								<div class="col-md-3">
									<label>Select Currency <span>*</span></label>
									<select id="SelectCurrency4" name="SelectCurrency4" class="choose-currency form-control select2" style="width: 100%;"></select>
								</div>
								<div class="col-md-3 d_none" id="currencytypediv4">
									<label>Indicate Currency Type <span>*</span></label>
									<input type="text" class="form-control" name="InputCurrencyType4" id="InputCurrencyType4" placeholder="Indicate Currency Type" autocomplete="off">
								</div>
								<div class="col-md-3">
									<label>Account Number <span>*</span></label>
									<input type="text" class="form-control" name="SettlementAccount4" id="SettlementAccount4" value="" placeholder="Account Number">
								</div>
								<div class="form-wizard-buttons m_top_20">
									<button type="button" class="btn btn-success" style="margin: 8px 15px 0px 0px;" id="btnAddAccount5"><span class="glyphicon glyphicon-plus-sign"></span> Add Another Account</button>
									<button type="button" class="d_none btn btn-danger" style="margin: 8px 15px 0px 0px;" id="btnRemoveAccount4"><span class="glyphicon glyphicon-minus-sign"></span> Remove Account</button>
								</div>
							</div>
							<div class="row d_none" id="AccountDiv5">
								<div class="col-md-3">
									<label>Select Currency <span>*</span></label>
									<select id="SelectCurrency5" name="SelectCurrency5" class="choose-currency form-control select2" style="width: 100%;"></select>
								</div>
								<div class="col-md-3 d_none" id="currencytypediv5">
									<label>Indicate Currency Type <span>*</span></label>
									<input type="text" class="form-control" name="InputCurrencyType5" id="InputCurrencyType5" placeholder="Indicate Currency Type" autocomplete="off">
								</div>
								<div class="col-md-3">
									<label>Account Number <span>*</span></label>
									<input type="text" class="form-control" name="SettlementAccount5" id="SettlementAccount5" value="" placeholder="Account Number">
								</div>
								<div class="col-md-3 m_top_10">
									<button type="button" class="d_none btn btn-danger" style="margin: 8px 15px 0px 0px;" id="btnRemoveAccount5"><span class="glyphicon glyphicon-minus-sign"></span> Remove Account</button>
								</div>
							</div><hr />
							<h5 class="d_none" id="disclaimer"><i>The counterparty hereby indemnifies the Bank against all and any losses and any demages that the Bank may suffer as a result of settling any transaction in any of the aforementioned bank accounts.</i></h5>
							<div class="row">
								<div class="col-md-6 pull-right">
									<div class="form-wizard-buttons">
										<button type="button" class="btn btn-save-settlement"><i class="fa fa-save"></i> Save</button>
										<button type="button" class="btn btn-success btn-edit-settlement d_none"><i class="fa fa-edit"></i> Change Accounts </button>
										<button type="button" class="btn btn-previous"><i class="fa fa-angle-left"></i> Back</button>
										<button type="button" class="btn btn-next twende-mbali"> Next <i class="fa fa-angle-right"></i></button>
									</div>
								</div>
							</div>
						</div>
					</fieldset>
					<!-- Form Step 3 -->
					<!-- Form Step 4 -->
					<fieldset>
						<h4>Authorized Signatories <span>Step 4 - 6</span></h4>
						<h5 style="font-size: 12px;">
							The following representatives of the counterparty are hereby designated as Authorized Signatories of the counterparty, having the authority to sign any relevant document pertaining to these terms on behalf of the Counterparty.
						</h5><hr />
						@if (ViewBag.ClientSignatory.Count > 0)
						{
							int nombre1 = 1;
							<table class="custom-table" id="signatories-table">
								<h5><b> Client Signatories. </b></h5>
								<th>#</th>
								<th> Full Names</th>
								<th> Designation </th>
								<th> Phone </th>
								<th> Email </th>
								<th> Uploaded Signature </th>
								@foreach (var item in ViewBag.ClientSignatory)
								{
									<tbody>
										<td>@nombre1</td>
										<td>@(item.Surname + " " + item.OtherNames)</td>
										<td>@item.Designation</td>
										<td>@item.PhoneNumber</td>
										<td>@item.EmailAddress</td>
										<td>
                                            @if (item.Signature == null || item.Signature == "")
                                            {
                                                <img width="100" height="80" src="~/Content/images/signatures/no-signature-17.jpg" />
                                            }
                                            else
                                            {
                                                <img width="100" height="80" src="~/Content/images/signatures/@item.Signature" />
                                            }
                                        </td>
									</tbody>
									nombre1 = nombre1 + 1;
								}
							</table><hr/>
						}
						else
						{
							<h5><b> Indicate Settlement Accounts. </b></h5>
						}
						<div id="SignatoryDiv1">
							<div class="panel panel-default">
								<div class="panel-heading">
									<span class="glyphicon glyphicon-user"></span> Account Signatory 1: <i>[My Details]</i>
								</div>
								<div class="panel-body">
									<div class="row">
										<div class="col-md-3">
											<label>Surname <span>*</span></label>
											<input type="text" class="form-control required" name="SignatorySurname1" id="SignatorySurname1" value="@ViewData["Signatory1Surname"]" placeholder="Surname">
										</div>
										<div class="col-md-3">
											<label>Other Names <span>*</span></label>
											<input type="text" class="form-control required" name="SignatoryOtherNames1" id="SignatoryOtherNames1" value="@ViewData["Signatory1OtherNames"]" placeholder="Other Names">
										</div>
										<div class="col-md-3">
											<label>Designation: <span>*</span></label>
											<input type="text" class="form-control required" name="SignatoryDesignation1" id="SignatoryDesignation1" value="@ViewData["Designation"]" placeholder="Signatory Designation">
										</div>
										<div class="col-md-3">
											<label>Email Address: <span>*</span></label>
											<input type="email" class="form-control required textToLower" name="SignatoryEmail1" id="SignatoryEmail1" value="@ViewData["CompanyEmail"]" placeholder="Email Address" readonly>
										</div>
									</div><br />
									<div class="row">
										<div class="col-md-3">
											<label>Phone Number: <span>*</span></label>
											<input type="text" class="form-control required" name="SignatoryPhoneNumber1" id="SignatoryPhoneNumber1" value="@ViewData["Signatory1Phone"]" placeholder="Include Country Code">
										</div>
										<div class="col-lg-4">
											<label>Upload Scanned Signature: <span>*</span> <i> Image Size (100 by 80)</i></label>
											<div class="form-group image-upload">
												<div class="setting image_picker">
													<div class="settings_wrap">
														<label class="drop_target">
															<div class="image_preview"><img src=""></div>
															<input id="inputFile" class="form-control required" type="file" name="inputFile" />
														</label>
														<div class="settings_actions vertical" style="margin:-70px 0px 0px 125px;">
															<a class="disabled" data-action="remove_current_image">
																<i class="fa fa-trash" aria-hidden="true"></i>
																Remove Signature
															</a>
														</div>
													</div>
												</div>
											</div>
										</div>
										<div class="col-lg-4 m_top_30">
											<h5><b>Image types to be uploaded</b></h5>
											<ul>
												<li style="color:red"><small>JPEG Image (.jpeg)</small></li>
												<li style="color:red"><small>JPEG Image (.jpg)</small></li>
												<li style="color:red"><small>Portable Network Graphic (.png)</small></li>
											</ul>
										</div>
										<div class="form-wizard-buttons m_top_20">
											<button type="button" class="btn btn-danger d_none" id="btnRemoveSignatory1"><span class="glyphicon glyphicon-minus-sign"></span> Remove </button>
											<button type="button" class="btn btn-success" style="margin: 8px 15px 0px 0px;" id="btnAddSignatory2"><span class="glyphicon glyphicon-plus-sign"></span> Add Signatory</button>
										</div>
									</div><br />
								</div>
							</div>
						</div>
						<div id="SignatoryDiv2" class="d_none">
							<div class="panel panel-default">
								<div class="panel-heading">
									<span class="glyphicon glyphicon-user"></span> Account Signatory 2
								</div>
								<div class="panel-body">
									<div class="row">
										<div class="col-md-3">
											<label>Surname <span>*</span></label>
											<input type="text" class="form-control" name="SignatorySurname2" id="SignatorySurname2" placeholder="Surname">
										</div>
										<div class="col-md-3">
											<label>Other Names <span>*</span></label>
											<input type="text" class="form-control" name="SignatoryOtherNames2" id="SignatoryOtherNames2" placeholder="Other Names">
										</div>
										<div class="col-md-3">
											<label>Designation: <span>*</span></label>
											<input type="text" class="form-control" name="SignatoryDesignation2" id="SignatoryDesignation2" placeholder="Designation">
										</div>
										<div class="col-md-3">
											<label>Email Address: <span>*</span></label>
											<input type="email" class="form-control textToLower" name="SignatoryEmail2" id="SignatoryEmail2" placeholder="Email Address">
										</div>
										<div class="col-md-3">
											<label>Phone Number: <span>*</span></label>
											<input type="text" class="form-control" name="SignatoryPhoneNumber2" id="SignatoryPhoneNumber2" placeholder="Include Country Code">
										</div>
									</div>
